# GoatFun Contracts Environment Variables
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# GOAT Testnet3 RPC URL
GOAT_RPC_URL=https://rpc.testnet3.goat.network

# Token address (tGOATED for testnet)
TOKEN_ADDRESS=0xa1FB4a8CEd8f916F4403ABFE65fD34Db564eEC9D

# Market Factory address (set after deployment)
MARKET_FACTORY_ADDRESS=

# Oracle address (set after deployment)
ORACLE_ADDRESS=

# Example values for testing:
# PRIVATE_KEY=04336ab74849dcdcc8f4df24c8a8a26d6e3cad627bf78290bb64f193222a7369
# TOKEN_ADDRESS=0xa1FB4a8CEd8f916F4403ABFE65fD34Db564eEC9D
